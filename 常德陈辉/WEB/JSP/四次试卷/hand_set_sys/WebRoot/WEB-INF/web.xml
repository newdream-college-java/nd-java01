<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>QueryHandSetServlet</servlet-name>
    <servlet-class>com.servlet.QueryHandSetServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FindByIdServlet</servlet-name>
    <servlet-class>com.servlet.FindByIdServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SelectHandSel</servlet-name>
    <servlet-class>com.servlet.SelectHandSel</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>QueryHandSetServlet</servlet-name>
    <url-pattern>/QueryHandSetServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FindByIdServlet</servlet-name>
    <url-pattern>/FindByIdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SelectHandSel</servlet-name>
    <url-pattern>/SelectHandSel</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
