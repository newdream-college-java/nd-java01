<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="newdream">
	<sql id="qb">
		jdid,jd,qxid
	</sql>
	<select id="list" resultType="fwjd">
		select <include refid="qb"/> from fwjd
	</select>
	<insert id="add" parameterType="entity.Fwjd">
		<selectKey keyProperty="jdid" order="AFTER" resultType="int" >
			select last_insert_id()
		</selectKey>
		insert into fwjd 
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="jd!=null">
				jd,
			</if>
			<if test="qxid!=null">
				qxid,
			</if>
						</trim>
			values
			<trim prefix="(" suffix=")" suffixOverrides=",">
				<if test="jd!=null">
				#{jd},
			</if>
			<if test="qxid!=null">
				#{qxid},
			</if>
		</trim>
	</insert>
	<update id="update" parameterType="entity.Users">
				update fwjd 
			<set>
				<if test="jd!=null">
				jd=#{jd},
			</if>
			<if test="qxid!=null">
				qxid=#{qxid},
			</if>
			</set>
			where jdid=#{jdid}
	</update>
</mapper>