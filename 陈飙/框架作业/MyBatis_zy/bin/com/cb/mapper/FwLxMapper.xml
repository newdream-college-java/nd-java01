<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cb.mapper.FwLxMapper">
	<update id="updateFW" parameterType="fwlx">
		update fwlx
		<set>
			<if test="lxid!=null">
				lxid=#{lxid},
  </if>
			<if test="fwlx!=null">
				fwlx=#{fwlx},
  </if>
		</set>
		where lxid=#{lxid}
	</update>
</mapper>