<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cb.mapper.FwJdMapper">
	<insert id="insertFw" parameterType="fwjd">
		insert into fwjd
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="jdid!=null">
				jdid,
  </if>
			<if test="jd!=null">
				jd,
  </if>
			<if test="qxid!=null">
				qxid,
  </if>
		</trim>
		values
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="jdid!=null">
				#{jdid},
  </if>
			<if test="jd!=null">
				#{jd},
  </if>
			<if test="qxid!=null">
				#{qxid},
  </if>
		</trim>
	</insert>
</mapper>